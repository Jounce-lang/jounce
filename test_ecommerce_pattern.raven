struct Product {
    stock: i32
}

fn test() {
    let product = Product { stock: 5 };
    let in_stock = product.stock > 0;

    return <div class="stock">
        {in_stock ?
            <span class="in-stock">{product.stock} in stock</span> :
            <span class="out-of-stock">Out of stock</span>
        }
    </div>;
}
