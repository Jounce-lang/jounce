// Test object literal support
fn main() {
    // Empty object
    let empty = {};

    // Object with fields
    let post = {
        id: 1,
        title: "Hello World",
        published: true
    };

    // Object with nested values
    let user = {
        name: "Alice",
        age: 30,
        email: "alice@example.com"
    };

    // Array of objects
    let posts = [
        { id: 1, title: "First Post" },
        { id: 2, title: "Second Post" },
        { id: 3, title: "Third Post" }
    ];

    console::log("Object literal test complete!");
}
