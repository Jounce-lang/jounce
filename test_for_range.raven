// Test file for range-based for loops
// Tests both exclusive (..) and inclusive (..=) ranges

fn main() {
    // Test exclusive range: 1..5 (should print 1, 2, 3, 4)
    println!("Exclusive range 1..5:");
    for i in 1..5 {
        println!("  i = {}", i);
    }

    // Test inclusive range: 1..=5 (should print 1, 2, 3, 4, 5)
    println!("Inclusive range 1..=5:");
    for i in 1..=5 {
        println!("  i = {}", i);
    }

    // Test range with expressions
    let start = 10;
    let end = 15;
    println!("Range with variables {}..{}:", start, end);
    for j in start..end {
        println!("  j = {}", j);
    }
}
