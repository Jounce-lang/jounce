// Test: Collections (Vec, HashMap, HashSet)
fn main() {
    // Vec operations
    let nums = vec![1, 2, 3, 4, 5];
    nums.push(6);
    let first = nums[0];
    let len = nums.len();

    // Vec iterators
    let doubled = nums.map(|x| x * 2);
    let evens = nums.filter(|x| x % 2 == 0);
    let sum = nums.reduce(|a, b| a + b, 0);

    // Array spread
    let combined = vec![...nums, 7, 8, 9];

    // Slice
    let subset = nums[1..3];
    let inclusive = nums[1..=3];

    // HashSet
    let set = HashSet::new();
    set.insert(1);
    set.insert(2);
    let has_one = set.contains(1);

    return 0;
}
