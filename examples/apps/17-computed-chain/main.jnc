// App 17: Chained Computed Values
component App() {
    let width = signal(10);
    let height = signal(20);
    let area = computed(() => width.value * height.value);
    let perimeter = computed(() => 2 * (width.value + height.value));

    <div>
        <p>Area: {area.value}</p>
        <p>Perimeter: {perimeter.value}</p>
    </div>
}
