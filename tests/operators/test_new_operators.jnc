fn test_logical_assignment() {
    // Test logical assignment operators (||=, &&=, ??=)
    let x = 0;
    let y = 10;
    let z = null;

    x ||= 10;  // x is falsy, so x = 10
    y &&= 5;   // y is truthy, so y = 5
    z ??= 0;   // z is null, so z = 0

    return x + y + z;
}
