// Test event handler support

fn handleClick() {
    console.log("Button clicked!");
}

fn App() -> JSX {
    return (
        <div>
            <h1>Event Handler Test</h1>
            <button onclick={handleClick}>Click Me</button>
        </div>
    );
}

fn main() {
    let app = App();
    console.log("App created");
}
