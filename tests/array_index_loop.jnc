// Regression test for array indexing with loop variables

fn test_array_index() {
    let items = vec![1, 2, 3, 4, 5];

    // Test 1: Range loop with array indexing
    for i in 0..items.len() {
        let x = items[i];
        console.log(x);
    }

    // Test 2: Nested array access
    let matrix = vec![vec![1, 2], vec![3, 4]];
    for i in 0..matrix.len() {
        for j in 0..matrix[i].len() {
            let val = matrix[i][j];
            console.log(val);
        }
    }
}

fn main() {
    test_array_index();
}
